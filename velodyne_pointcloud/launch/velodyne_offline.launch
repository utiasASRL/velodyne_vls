<!-- -*- mode: XML -*- -->
<!-- run velodyne_pointcloud/cloud_node as a node

     arg: calibration = path to calibration file (default: utias_db.yaml.yaml)


  -->

<launch>
  <!-- declare arguments with default values -->
  <!--arg name="calibration" default="$(find asrl__lasers__velodyne_config)/params/asrl_64e_v2.yaml"/-->
  <!-- <arg name="calibration" default="$(find velodyne_pointcloud)/params/utias_db.yaml"/> -->
  <!-- <arg name="calibration" default="$(find velodyne_pointcloud)/params/vel_64_calib.yaml"/> -->
  <!-- <arg name="calibration" default="$(find velodyne_pointcloud)/params/64e_s3-xiesc.yaml"/> -->
  <arg name="calibration" default="$(find velodyne_pointcloud)/params/VeloView_VLS-128_FS1.yaml"/>
  <arg name="max_range" default="300.0" />
  <arg name="min_range" default="0.4" />

  <node name="cloud_node" type="cloud_node" pkg="velodyne_pointcloud" output="screen">
    <param name="calibration" value="$(arg calibration)" />
    <param name="max_range" value="$(arg max_range)"/>
    <param name="min_range" value="$(arg min_range)"/>
  </node>

</launch>
